{
  "manifest_version": 3,
  "name": "MXL Web",
  "version": "1.0",
  "description": "Enhances the MXL website experience.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "48": "./images/icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content.js"]
    }
  ]
}